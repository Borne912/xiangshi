/* miniprogram/pages/health/health.wxss */
.cover{
  width: 750rpx;
  height: 100%;
  line-height: 40rpx;
  border-radius: 0rpx;
  background-color: rgba(255, 242, 232, 1);
  text-align: center;
  overflow: hidden;
}

.data-wrapper{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-top: 34rpx;
  width: 750rpx;
  height: 50rpx;
}

.data-wrapper .label-wrapper text{
  color: rgba(77, 63, 63, 1);
  font-size: 32rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}

.data-wrapper .time-wrapper{
  line-height: 40rpx;
  border-radius: 20rpx;
  background-color: rgba(255, 161, 85, 1);
  text-align: center;
  border: 2rpx solid rgba(255, 255, 255, 0);
}

.data-wrapper .time-wrapper text{
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}

.PieChart-wrapper{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  margin-top: 46rpx;
  width: 750rpx;
  height: 200rpx;
}

.PieChart-wrapper .PieChart{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 200rpx;
  height: 200rpx;
  line-height: 40rpx;
  background-color: rgba(255, 161, 85, 1);
  text-align: center;
  border: 2rpx solid rgba(255, 255, 255, 0);
  border-radius: 50%;
}

.PieChart-wrapper .PieChart .label text{
  color: rgba(77, 63, 63, 1);
  font-size: 40rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}

.PieChart-wrapper .PieChart .percent text{
  color: rgba(77, 63, 63, 1);
  font-size: 26rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}

.td-food-wrapper{
  position: relative;
  margin: 32rpx 32rpx 0rpx 30rpx;
  height: 280rpx;
  width: 750rpx;
}

.td-food-wrapper .tag{
  position: absolute;
  width: 154rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 20rpx;
