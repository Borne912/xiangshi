.td-food-wrapper .content .elem{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  /* position: absolute; */
  margin-top: 52rpx;
  width: 688rpx;
  height: 156rpx;
}

.td-food-wrapper .content .elem .demo{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 198rpx;
  height: 156rpx;
  line-height: 40rpx;
  border-radius: 30rpx 30rpx 30rpx 30rpx;
  background-color: rgba(255, 230, 210, 1);
  text-align: center;
  border: 2rpx solid rgba(255, 255, 255, 0);
}

.td-food-wrapper .content .elem .demo text{
  color: rgba(77, 63, 63, 1);
  font-size: 28rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}

.td-food-wrapper .content .icon{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-top: 4rpx;
  width: 688rpx;
  height: 48rpx;
}

.td-food-wrapper .content .icon .image image,
.td-food-wrapper .content .icon .image{
  width: 48rpx;
  height: 48rpx; 
}

.td-food-wrapper .content .icon .image image{
  transform: rotate(90deg); /*图片旋转90度*/
}

